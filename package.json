{
  "name": "@xyz/nuxt-confirm",
  "version": "1.0.1",
  "description": "A promise-based confirmation dialog module for Nuxt applications that provides customizable, accessible modal confirmations",
  "repository": "https://github.com/xyzrepo/nuxt-confirm",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.15.4",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.1.3",
    "@nuxt/eslint-config": "^1.1.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.17.1",
    "@nuxt/ui": "^2.21.0",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.21.0",
    "nuxt": "^3.15.4",
    "typescript": "~5.8.2",
    "vitest": "^3.0.7",
    "vue-tsc": "^2.2.8"
  },
  "packageManager": "pnpm@8.9.2+sha1.5f2fa48d614263457cf5d7fb7be8b878da318d87"
}
